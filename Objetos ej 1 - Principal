Scanner lector = new Scanner(System.in);
        
		Cuenta_bancaria cuenta = new Cuenta_bancaria();
		// Llama al método contructor para generar el objeto
		
		// Variables - "cuenta"
		int monto_usuario;
		int numero_usuario;
		int cedula_usuario;
		
		
		System.out.println("Ingrese monto");
		monto_usuario = lector.nextInt();
		
		cuenta.Cargar_monto(monto_usuario);
		// Llama al método "Cargar_monto" con "monto_usuario"
		
		System.out.println("El monto de su cuenta es " + cuenta.Obtener_monto());
		// Llama al método "Obtener_monto" y lo muestra en pantalla
				
        System.out.println("Ingrese el número de su cuenta");
        numero_usuario = lector.nextInt();
        
		cuenta.Cargar_numero(numero_usuario);
		// Llama al método "Cargar_numero" con "numero_usuario"
		
		System.out.println("El número de su cuenta es " + cuenta.Obtener_numero());
		// Llama al método "Obtener_numero" y lo muestra en pantalla
		
		System.out.println("Ingrese la cédula de su cuenta");
		cedula_usuario = lector.nextInt();
		
		cuenta.Cargar_cedula(cedula_usuario);
		// Llama al método "Cargar_cedula" con "cedula_usuario"
		
		System.out.println("La cédula de su cuenta es " + cuenta.Obtener_cedula());
		// Llama al método "Obtener_cedula" y lo muestra en pantalla
		lector.close();
	}

}
