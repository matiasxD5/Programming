private int numero;
	private int cedula;
	private int monto;
	
	/* Constructor inicializa en 0 todos los valores
	* no recibe parametros
	*/
	public Cuenta_bancaria() {
		
		 numero = 0;
	     cedula = 0;
		 monto = 0;
	}
	/* Método "SETTER", para cargar monto
	* @param a es un entero que representa el monto que se agregará
	* @return void
	*/
	public void Cargar_monto(int monto) {
		
		this.monto = monto;
	}
	/* Método "GETTER", para obtener monto
	* no recibe parametros
	* @return un entero que es el monto de la cuenta
	*/
	public int Obtener_monto() {
		
		return monto;
	}
	/* Método "SETTER", para cargar número de la cuenta
	* @param a es un entero que representa el número que se agregará
	* @return void
	*/
public void Cargar_numero(int numero) {
		
		this.numero = numero;
	}
	/* Método "GETTER", para obtener el número de la cuenta
	* no recibe parametros
	* @return un entero que es el número de la cuenta
	*/
	public int Obtener_numero() {
		
		return numero;
	}
	/* Método "SETTER", para cargar cédula de la cuenta
	* @param a es un entero que representa el número (cédula) que se agregará
	* @return void
	*/
public void Cargar_cedula(int cedula) {
		
		this.cedula = cedula;
	}
	/* Método "GETTER", para obtener el número de cédula de la cuenta
	* no recibe parametros
	* @return un entero que es el número de cédula de la cuenta
	*/
	public int Obtener_cedula() {

		
		return cedula;
	}
    public void cuenta(int cedula, int numero, int monto) {
	
    	this.cedula = cedula;
    	this.numero = numero;
    	this.monto = monto;
	}
	
	
}
